---
const { class: className = "size-6 text-white" } = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  viewBox="0 0 32 32"
  fill="none"
  stroke="currentColor"
  stroke-width="2"
  aria-hidden="true"
  class={className}
>
  <ellipse rx="5.5" ry="13" transform="rotate(60 16 16)" cx="16" cy="16" />
  <ellipse rx="5.5" ry="13" transform="rotate(-60 16 16)" cx="16" cy="16" />
  <ellipse rx="5.5" ry="13" cx="16" cy="16" />
  <circle cx="16" cy="16" r="2" fill="currentColor" stroke="none" />
</svg>
